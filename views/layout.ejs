<body>
  <%- include('_header'); %>

    <%- body %>
</body>
<script>
           <%if (flash.success && flash.success.length > 0) {%>
              new Noty({
                theme:'relax',
              text:"<%= flash.success%>",
              type:'success',
              layout:"topRight",
              timeout:1500

             }).show();
       
          <%}%>
          <%if (flash.error && flash.error.length > 0) {%>
              new Noty({
                theme:'relax',
              text:"<%= flash.success%>",
              type:'success',
              layout:"topRight",
              timeout:1500

             }).show();
       
          <%}%>
</script>


</html>